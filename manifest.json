{
  "manifest_version": 3,
  "name": "Custom Bookmark Organizer",
  "version": "1.0",
  "description": "Organize the bookmark visually with categories and tags",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["bookmarks", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}
